
#header {
  width: 100%;
  height: 10vh;
  border-bottom: 2rpx solid #ccc;
  box-sizing: border-box;
}

#main {
  height: 90vh;
}
.weui-navbar__item.weui-bar__item_on {
  color: #f00;
}

.img-container {
  text-align: center;
  position: relative;
  border-radius: 10rpx;
  overflow: hidden;
}
.yuedu{
  box-sizing: border-box;
  font-size: 22rpx;
  color: #fff;
  padding:0 20rpx;
  background: rgba(0, 0, 0, 0.5);
  width:116rpx;
  height: 60rpx;
  border-radius: 30rpx;
  overflow: hidden;
  line-height: 60rpx;
  position: absolute;
  right: 20rpx;
  bottom:20rpx;

}
.img-size {
  width: 690rpx;
  height: 460rpx;
  vertical-align: bottom;
}

.pro-container {
  margin: 50rpx 30rpx;
}

.pro-title {
  padding: 20rpx 0 9rpx;
  font-size: 30rpx;
}

.pro-title2 {
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  width: 500rpx;
}

.pro-title1 {
  overflow: hidden;
  width: 500rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  color: #999;
  font-size: 24rpx;
}

.pro-btn {
  color: #fff;
  background: #e51c23;
  font-size: 24rpx;
  padding: 6rpx 33rpx;
}

.pro-end {
  font-size: 24rpx;
  color: #999;
}

.pro-end1 {
  margin: 0 20rpx;
  width: 160rpx;
  height: 2rpx;
  background: rgba(204, 204, 204, 1);
}
.zhiding {
  width: 90rpx;
  height: 90rpx;
  background: rgba(0, 0, 0, 0.3);
  border-radius: 50%;
  position: fixed;
  right: 10rpx;
  bottom: 200rpx;
}

.zhiding1 {
  width: 50%;
  height: 50%;
}