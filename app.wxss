/**app.wxss**/
@import 'public/weui.wxss';
.nowrap{
  white-space: nowrap;
}
.hr{
  height: 20rpx;
  background: #ccc;
}
.full-img{
  width:100%;
  height: 100%;
}
.text-center{
  text-align: center;
}
.flex-start {
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-start;
  align-items: center;
}
.flex-end {
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-end;
  align-items: center;
}
.flex-between {
  display: flex;
   flex-wrap: wrap;
  justify-content: space-between;
  align-content: space-between;
  align-items: center;
}
.flex-center {
  display: flex;
  justify-content: center;
  align-content: center;
  align-items: center;
}
